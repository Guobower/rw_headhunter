<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- [START] - personnel ============================================================================ -->
        <!-- form - simple -->
        <record id="personnel_view_form1" model="ir.ui.view">
            <field name="name">personnel_view_form1</field>
            <field name="model">rw_headhunter.personnel</field>
            <field name="arch" type="xml">
                <form string="Fast Create">
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>
        <!-- form -->
        <record id="personnel_view_form" model="ir.ui.view">
            <field name="name">personnel_view_form_inherit</field>
            <field name="model">rw_headhunter.personnel</field>
            <field name="inherit_id" ref="rw_headhunter.baseattributeimage_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="before">
                    <separator/>
                    <group string="Information">
                        <group>
                            <field name="gender"/>
                        </group>
                        <group>
                            <field name="mobile"/>
                            <field name="mail"/>
                        </group>
                    </group>
                    <separator/>
                </xpath>
            </field>
        </record>
        <!-- tree -->
        <record id="personnel_view_tree" model="ir.ui.view">
            <field name="name">personnel_view_tree_inherit</field>
            <field name="model">rw_headhunter.personnel</field>
            <field name="inherit_id" ref="rw_headhunter.baseattributeimage_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="mobile"/>
                    <field name="mail"/>
                </xpath>
            </field>
        </record>
        <!-- actions -->
        <record id="personnel_action_view_tree" model="ir.actions.act_window">
            <field name="name">personnel_action_view_tree</field>
            <field name="res_model">rw_headhunter.personnel</field>
            <field name="view_ids"
                   eval="[(5, 0, 0),
                          (0, 0, {'view_mode': 'tree', 'view_id': ref('personnel_view_tree')}),
                          (0, 0, {'view_mode': 'form', 'view_id': ref('personnel_view_form')})
                          ]"/>
        </record>
        <!-- menu-categories-list -->
        <menuitem id="menu_list_personnel" name="personnel list" parent="menu_122"
                  action="personnel_action_view_tree"/>
        <!-- [END] - personnel ============================================================================== -->

